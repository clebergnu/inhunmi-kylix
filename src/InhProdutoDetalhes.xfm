  object ComprasDSet: TSQLClientDataSet
    CommandText = 
      'SELECT estoque_movimento_produto_grupo.id AS grupo_id, estoque_m' +
      'ovimento_produto.id AS movimento_id, pessoa_instituicao.nome AS ' +
      'fornecedor, estoque_movimento_produto.quantidade, estoque_movime' +
      'nto_produto.valor AS valor_total, (estoque_movimento_produto.val' +
      'or / estoque_movimento_produto.quantidade) AS valor_unitario, es' +
      'toque_movimento_produto.datahora FROM estoque_movimento_produto_' +
      'grupo, estoque_movimento_produto, pessoa_instituicao WHERE (pess' +
      'oa_instituicao.id = estoque_movimento_produto.fornecedor) AND (e' +
      'stoque_movimento_produto.produto = :produto_id) AND ((estoque_mo' +
      'vimento_produto.grupo IS NULL) OR (estoque_movimento_produto.gru' +
      'po = estoque_movimento_produto_grupo.id))'
    Aggregates = <>
